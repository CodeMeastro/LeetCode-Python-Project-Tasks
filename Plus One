class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        if not digits:
            return digits
        listing = []
        string = ""

        for i in range(len(digits)):
            string += str(digits[i])
        
        digit = int(string)+1
        for i in str(digit):
            listing.append(int(i))
        return listing 
